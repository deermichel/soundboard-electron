{
    "compilerOptions": {
        "allowJs": false,
        "module": "commonjs",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "sourceMap": true,
        "baseUrl": "src",
        "outDir": "dist",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react-jsx",
        "paths": {}
    },
    "include": [
        "src"
    ]
}
